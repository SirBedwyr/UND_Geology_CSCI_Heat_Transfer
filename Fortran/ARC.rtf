\par \hich\af2\dbch\af37\loch\f2       SUBROUTINE CONV
\par \hich\af2\dbch\af37\loch\f2       COMMON /FIR\hich\af2\dbch\af37\loch\f2 ST/ X,Y,NR,NC,T,TI,NOD
\par \hich\af2\dbch\af37\loch\f2       COMMON /THIRD/ NODCV,RVSH,FVSH,TPMIN,TIC,VEL,TOTIM,KSAVE,TP0
\par \hich\af2\dbch\af37\loch\f2       REAL W(120,120),T(120,120),FVSH(8),RVSH(8),X(120)
\par \hich\af2\dbch\af37\loch\f2       REAL TPMIN(9),Y(120),VEL(8)
\par \hich\af2\dbch\af37\loch\f2       REAL TI,TA,TIC,XB,YB,CT,AMT,DIST,RATIO
\par \hich\af2\dbch\af37\loch\f2       INTEGER NODCV(120,12\hich\af2\dbch\af37\loch\f2 0), NOD(120,120)
\par \hich\af2\dbch\af37\loch\f2       INTEGER I,J,NR,NC,J1,I1,LCONV,HC,IHCVN,MT,NN,KF,KV,KR,I2
\par \hich\af2\dbch\af37\loch\f2       I2 = 1
\par \hich\af2\dbch\af37\loch\f2       LCONV = INT(TI / (10 * TIC))
\par \hich\af2\dbch\af37\loch\f2       IF (LCONV.GT.5) THEN 
\par \hich\af2\dbch\af37\loch\f2          LCONV = 5
\par \hich\af2\dbch\af37\loch\f2       ENDIF
\par \hich\af2\dbch\af37\loch\f2  5030  IF (LCONV.LE.0) THEN 
\par \hich\af2\dbch\af37\loch\f2           LCONV = 1
\par \hich\af2\dbch\af37\loch\f2        ENDI\hich\af2\dbch\af37\loch\f2 F
\par \hich\af2\dbch\af37\loch\f2 C       WRITE(*,*) LCONV
\par \hich\af2\dbch\af37\loch\f2  5050 TA = TI / REAL(LCONV)
\par \hich\af2\dbch\af37\loch\f2       DO 5850 IHCVN = 1, LCONV
\par \hich\af2\dbch\af37\loch\f2 C        HC = 0
\par \hich\af2\dbch\af37\loch\f2 C        NCC = 0
\par \hich\af2\dbch\af37\loch\f2 C        I2 = I2+1
\par \hich\af2\dbch\af37\loch\f2  5080    DO  I = 1, NR
\par \hich\af2\dbch\af37\loch\f2            DO  J = 1, NC
\par 
\par \hich\af2\dbch\af37\loch\f2 C               WRITE(*,*) 'ROW',I,'COLUMN',J
\par \hich\af2\dbch\af37\loch\f2             \hich\af2\dbch\af37\loch\f2             IF (NODCV(I, J)) 5110, 5110, 5130
\par \hich\af2\dbch\af37\loch\f2  5110                   W(I, J) = T(I, J)
\par \hich\af2\dbch\af37\loch\f2                         GO TO 5700
\par \hich\af2\dbch\af37\loch\f2  5130                   KK = NOD(I, J) / 100
\par \hich\af2\dbch\af37\loch\f2                         KK = NOD(I, J) - 100 * KK
\par \hich\af2\dbch\af37\loch\f2                         IF (KK - \hich\af2\dbch\af37\loch\f2 2) 5160, 5110, 5160
\par \hich\af2\dbch\af37\loch\f2  5160                   XB = 0.0
\par \hich\af2\dbch\af37\loch\f2                         YB = 0.0
\par \hich\af2\dbch\af37\loch\f2                         MT = NODCV(I, J) / 10000
\par \hich\af2\dbch\af37\loch\f2                         NN = NODCV(I, J) - 10000 * MT
\par \hich\af2\dbch\af37\loch\f2                         KV = NN / 1000
\par \hich\af2\dbch\af37\loch\f2                       \hich\af2\dbch\af37\loch\f2   NN = NN - KV * 1000
\par \hich\af2\dbch\af37\loch\f2                         KF = NN / 100
\par \hich\af2\dbch\af37\loch\f2                         NN = NN - KF * 100
\par \hich\af2\dbch\af37\loch\f2                         KR = NN / 10
\par \hich\af2\dbch\af37\loch\f2                         NN = NN - KR * 10
\par \hich\af2\dbch\af37\loch\f2 C        WRITE(*,*) NODCV(I,J),MT,KV,KF,KR,NN
\par 
\par \hich\af2\dbch\af37\loch\f2 c      GO TO(5270,53\hich\af2\dbch\af37\loch\f2 20,5340,5370,5410,5420,5450,5500,5520)NN
\par \hich\af2\dbch\af37\loch\f2         SELECT CASE (NN)
\par \hich\af2\dbch\af37\loch\f2                 CASE (1)
\par \hich\af2\dbch\af37\loch\f2  5270                    I1 = I - 1
\par \hich\af2\dbch\af37\loch\f2                          J1 = J 
\par \hich\af2\dbch\af37\loch\f2                 CASE (2)
\par \hich\af2\dbch\af37\loch\f2  5320                    I1 = I - 1
\par \hich\af2\dbch\af37\loch\f2                          J1 \hich\af2\dbch\af37\loch\f2 = J - 1
\par \hich\af2\dbch\af37\loch\f2                 CASE (3)
\par \hich\af2\dbch\af37\loch\f2  5340                    I1 = I - 1
\par \hich\af2\dbch\af37\loch\f2  5370                    J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2                 CASE (4)
\par \hich\af2\dbch\af37\loch\f2  5420                    I1 = I 
\par \hich\af2\dbch\af37\loch\f2  5450                    J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2                 CASE (5)
\par \hich\af2\dbch\af37\loch\f2                         \hich\af2\dbch\af37\loch\f2 GO TO 5615
\par \hich\af2\dbch\af37\loch\f2                 CASE (6)
\par \hich\af2\dbch\af37\loch\f2  5470                   I1 = I 
\par \hich\af2\dbch\af37\loch\f2                         J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2                 CASE (7)
\par \hich\af2\dbch\af37\loch\f2  5500                    I1 = I + 1
\par \hich\af2\dbch\af37\loch\f2  5520                    J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2                 CASE (8)
\par \hich\af2\dbch\af37\loch\f2  5560                  \hich\af2\dbch\af37\loch\f2   I1 = I + 1
\par \hich\af2\dbch\af37\loch\f2                          J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2  5570           CASE (9)                 
\par \hich\af2\dbch\af37\loch\f2                          I1 = I + 1
\par \hich\af2\dbch\af37\loch\f2                          J1 = J 
\par \hich\af2\dbch\af37\loch\f2                 CASE DEFAULT 
\par \hich\af2\dbch\af37\loch\f2                          GO TO 5615
\par \hich\af2\dbch\af37\loch\f2                 END SEL\hich\af2\dbch\af37\loch\f2 ECT
\par \hich\af2\dbch\af37\loch\f2                 
\par \hich\af2\dbch\af37\loch\f2                 IF (I1.GT.NR) I1 = NR-1
\par \hich\af2\dbch\af37\loch\f2                 IF (J1.GT.NC) J1 = NC -1 
\par \hich\af2\dbch\af37\loch\f2                 IF (I1.LE.0) I1 = 1
\par \hich\af2\dbch\af37\loch\f2                 IF (J1.LE.0) J1 = 1
\par 
\par \hich\af2\dbch\af37\loch\f2  
\par \hich\af2\dbch\af37\loch\f2  5610          IF (T(I1, J1) - TPMIN(MT)) 5615, 5620, 5620
\par \hich\af2\dbch\af37\loch\f2  5615     \hich\af2\dbch\af37\loch\f2      W(I, J) = T(I, J)
\par \hich\af2\dbch\af37\loch\f2                GO TO 5700
\par \hich\af2\dbch\af37\loch\f2  5620          XB = X(J1) + X(J)
\par \hich\af2\dbch\af37\loch\f2                YB = Y(I1) + Y(I)
\par \hich\af2\dbch\af37\loch\f2                CT = T(I1, J1) - T(I, J)
\par \hich\af2\dbch\af37\loch\f2                AMT = (VEL(KV) * FVSH(KF) / RVSH(KR)) * TA
\par \hich\af2\dbch\af37\loch\f2                DIST = SQRT((XB / 2.)*\hich\af2\dbch\af37\loch\f2 *2 + (YB / 2.)**2)
\par \hich\af2\dbch\af37\loch\f2                RATIO = AMT / DIST
\par \hich\af2\dbch\af37\loch\f2                IF (RATIO-1) 5690,5690, 5670 
\par \hich\af2\dbch\af37\loch\f2  5670          HC = 1
\par \hich\af2\dbch\af37\loch\f2                RATIO = .999999
\par \hich\af2\dbch\af37\loch\f2  5690          W(I, J) = T(I, J) + RATIO * CT
\par \hich\af2\dbch\af37\loch\f2  5700      END DO
\par \hich\af2\dbch\af37\loch\f2  5775   END DO
\par \hich\af2\dbch\af37\loch\f2         DO  I = 1,\hich\af2\dbch\af37\loch\f2  NR
\par \hich\af2\dbch\af37\loch\f2            DO  J = 1, NC
\par \hich\af2\dbch\af37\loch\f2                  T(I, J) = W(I, J)
\par \hich\af2\dbch\af37\loch\f2  5776      END DO
\par \hich\af2\dbch\af37\loch\f2  5777   END DO   
\par \hich\af2\dbch\af37\loch\f2 C   The following was a column counting scheme left over from Chuck 
\par \hich\af2\dbch\af37\loch\f2 c   Brott's original FORTRAN 2 code.  I have kept it for no particular
\par \hich\af2\dbch\af37\loch\f2 c   reaso\hich\af2\dbch\af37\loch\f2 n other than I was never certain exactly what it did.  Thus,
\par \hich\af2\dbch\af37\loch\f2 c   when in doubt, don't change anything.
\par \hich\af2\dbch\af37\loch\f2 C         IF (HC) 5860, 5860, 5780 
\par \hich\af2\dbch\af37\loch\f2 C 5780       IF (NCC-1) 5790, 5820, 5850
\par \hich\af2\dbch\af37\loch\f2 C 5790         IF (I2.LE.NC) NCC = 1
\par \hich\af2\dbch\af37\loch\f2 C              GOTO 5080
\par \hich\af2\dbch\af37\loch\f2 C 5820  \hich\af2\dbch\af37\loch\f2  NCC = 2
\par \hich\af2\dbch\af37\loch\f2 C        GOTO 5860
\par \hich\af2\dbch\af37\loch\f2  5850    CONTINUE
\par \hich\af2\dbch\af37\loch\f2  5860 RETURN
\par \hich\af2\dbch\af37\loch\f2       END
\par \hich\af2\dbch\af37\loch\f2 C*********************************       
\par \hich\af2\dbch\af37\loch\f2        SUBROUTINE SAVIT
\par \hich\af2\dbch\af37\loch\f2        COMMON SFILE,TITLE,SUFILE
\par \hich\af2\dbch\af37\loch\f2        COMMON IFCV
\par \hich\af2\dbch\af37\loch\f2        COMMON /FIRST/ X,Y,NR,NC,T,TI,NOD
\par \hich\af2\dbch\af37\loch\f2        COMMON /SEC\hich\af2\dbch\af37\loch\f2 OND/ D,CHP,DTC,CHF,TIME
\par \hich\af2\dbch\af37\loch\f2        COMMON /THIRD/ NODCV,RVSH,FVSH,TPMIN,TIC,VEL,TOTIM,KSAVE,TP0
\par \hich\af2\dbch\af37\loch\f2        
\par \hich\af2\dbch\af37\loch\f2        REAL X(120),Y(120),T(120,120)
\par \hich\af2\dbch\af37\loch\f2        REAL TI,TIME,TPMIN(9),VEL(8),TIC,TM,TN,RNCX,RNRY
\par \hich\af2\dbch\af37\loch\f2        REAL CHP(9),D(9),DTC,CHF,RVSH(8),FVSH(8),TP0
\par \hich\af2\dbch\af37\loch\f2      \hich\af2\dbch\af37\loch\f2   INTEGER NOD(120,120),NODCV(120,120),IFCV,KSAVE
\par \hich\af2\dbch\af37\loch\f2        
\par \hich\af2\dbch\af37\loch\f2        CHARACTER*80 SFILE,SUFILE
\par \hich\af2\dbch\af37\loch\f2        CHARACTER*80 TITLE
\par 
\par \hich\af2\dbch\af37\loch\f2        IF (KSAVE) 1220,1220,5
\par \hich\af2\dbch\af37\loch\f2     5  OPEN (11,FILE=SFILE, STATUS = 'UNKNOWN')
\par \hich\af2\dbch\af37\loch\f2    16  FORMAT(F6.2)
\par \hich\af2\dbch\af37\loch\f2        WRITE (11,12) NR, NC, IFCV
\par \hich\af2\dbch\af37\loch\f2        WRITE (11,11) CHF*1000,TIME
\par \hich\af2\dbch\af37\loch\f2   11   FORMAT(F6.2,F15.3)
\par \hich\af2\dbch\af37\loch\f2   12   FORMAT(3I4)
\par \hich\af2\dbch\af37\loch\f2        WRITE (11,15) TITLE
\par \hich\af2\dbch\af37\loch\f2    15  FORMAT(A80)
\par 
\par \hich\af2\dbch\af37\loch\f2        DO I = 1, NR
\par \hich\af2\dbch\af37\loch\f2          WRITE(11,21) (T(I,J),J=1,NC)
\par \hich\af2\dbch\af37\loch\f2        END DO
\par \hich\af2\dbch\af37\loch\f2    21  FORMAT(120F10.2)
\par \hich\af2\dbch\af37\loch\f2      
\par \hich\af2\dbch\af37\loch\f2        DO I = \hich\af2\dbch\af37\loch\f2 1, NR
\par \hich\af2\dbch\af37\loch\f2          WRITE(11,22) (NOD(I,J),J=1,NC)
\par \hich\af2\dbch\af37\loch\f2        END DO
\par \hich\af2\dbch\af37\loch\f2    22  FORMAT(120I6)
\par \hich\af2\dbch\af37\loch\f2    
\par \hich\af2\dbch\af37\loch\f2        IF (IFCV.EQ.1) THEN
\par \hich\af2\dbch\af37\loch\f2         DO I = 1,NR
\par \hich\af2\dbch\af37\loch\f2           WRITE(11,22) (NODCV(I,J),J=1,NC)
\par \hich\af2\dbch\af37\loch\f2         END DO
\par \hich\af2\dbch\af37\loch\f2        END IF
\par 
\par \hich\af2\dbch\af37\loch\f2        WRITE(11,23) (X(I),I=1,NC)
\par 
\par \hich\af2\dbch\af37\loch\f2    \hich\af2\dbch\af37\loch\f2 51  CONTINUE 
\par \hich\af2\dbch\af37\loch\f2    23  FORMAT(50F10.2)
\par \hich\af2\dbch\af37\loch\f2        WRITE(11,23) (Y(I),I=1,NR)
\par \hich\af2\dbch\af37\loch\f2        WRITE(11,24) ((CHP(I)*1E6),I=1,8)
\par \hich\af2\dbch\af37\loch\f2    24  FORMAT(8G10.2)
\par \hich\af2\dbch\af37\loch\f2        WRITE(11,24) ((D(I)/14.33),I=1,8)
\par \hich\af2\dbch\af37\loch\f2        IF (IFCV.EQ.1) THEN
\par \hich\af2\dbch\af37\loch\f2         WRITE(11,24) (FVSH(I),I=1,8)
\par \hich\af2\dbch\af37\loch\f2         W\hich\af2\dbch\af37\loch\f2 RITE(11,24) (RVSH(I),I=1,8)
\par \hich\af2\dbch\af37\loch\f2         WRITE(11,24) (TPMIN(I),I=1,8)
\par \hich\af2\dbch\af37\loch\f2         WRITE(11,24) (VEL(I),I=1,8)
\par \hich\af2\dbch\af37\loch\f2        END IF
\par \hich\af2\dbch\af37\loch\f2        CLOSE (11)
\par 
\par \hich\af2\dbch\af37\loch\f2  1220 IF (TIME.LT.TP0) THEN              
\par \hich\af2\dbch\af37\loch\f2          RETURN
\par \hich\af2\dbch\af37\loch\f2       ENDIF
\par \hich\af2\dbch\af37\loch\f2       OPEN (12,FILE=SUFILE,STATUS='UNKNOWN\hich\af2\dbch\af37\loch\f2 ')
\par \hich\af2\dbch\af37\loch\f2       WRITE(12,1222) 'DSAA'
\par \hich\af2\dbch\af37\loch\f2  1222 FORMAT(A4)
\par \hich\af2\dbch\af37\loch\f2       WRITE(12,1223) NC,NR
\par \hich\af2\dbch\af37\loch\f2       
\par \hich\af2\dbch\af37\loch\f2       RNCX=X(1)*real(NC)
\par \hich\af2\dbch\af37\loch\f2       RNRY=Y(1)*real(NR)
\par 
\par \hich\af2\dbch\af37\loch\f2       IF (X(1).LT.0.01) THEN
\par \hich\af2\dbch\af37\loch\f2         RNCX = RNCX*1000.
\par \hich\af2\dbch\af37\loch\f2       ELSE IF (X(1).LT.0.1) THEN
\par \hich\af2\dbch\af37\loch\f2         RNCX = RNCX*100.
\par \hich\af2\dbch\af37\loch\f2       ELSE IF (X(1).LT.1) THEN
\par \hich\af2\dbch\af37\loch\f2         RNCX = RNCX*10.
\par \hich\af2\dbch\af37\loch\f2       END IF
\par \hich\af2\dbch\af37\loch\f2       IF (Y(1).LT.0.01) THEN
\par \hich\af2\dbch\af37\loch\f2         RNRY = RNRY*1000.
\par \hich\af2\dbch\af37\loch\f2       ELSE IF (Y(1).LT.0.1) THEN
\par \hich\af2\dbch\af37\loch\f2         RNRY = RNRY*100.
\par \hich\af2\dbch\af37\loch\f2       ELSE IF (Y(1).LT.1) THEN
\par \hich\af2\dbch\af37\loch\f2         RNRY = RNRY*10.
\par \hich\af2\dbch\af37\loch\f2       END\hich\af2\dbch\af37\loch\f2  IF
\par \hich\af2\dbch\af37\loch\f2       WRITE(12,1224) 0.0,RNCX
\par \hich\af2\dbch\af37\loch\f2       WRITE(12,1224) -RNRY,0.0
\par \hich\af2\dbch\af37\loch\f2  1223 FORMAT(2I4)
\par \hich\af2\dbch\af37\loch\f2       TN = 1000.0
\par \hich\af2\dbch\af37\loch\f2       TM = -1000.0
\par \hich\af2\dbch\af37\loch\f2       DO I = 1,NR
\par \hich\af2\dbch\af37\loch\f2        DO J = 1,NC
\par \hich\af2\dbch\af37\loch\f2         IF (T(I,J).GT.TM) TM=T(I,J)
\par \hich\af2\dbch\af37\loch\f2         IF (TN.GT.T(I,J)) TN=T(I,J)
\par \hich\af2\dbch\af37\loch\f2        END DO
\par \hich\af2\dbch\af37\loch\f2   \hich\af2\dbch\af37\loch\f2     END DO
\par \hich\af2\dbch\af37\loch\f2       WRITE(12,1224) TN,TM
\par \hich\af2\dbch\af37\loch\f2  1224 FORMAT(2F10.2)
\par \hich\af2\dbch\af37\loch\f2        DO I =  NR,1,-1
\par \hich\af2\dbch\af37\loch\f2          WRITE(12,21) (T(I,J),J=1,NC)
\par \hich\af2\dbch\af37\loch\f2        END DO
\par 
\par \hich\af2\dbch\af37\loch\f2       RETURN
