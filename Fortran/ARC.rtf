\par \hich\af2\dbch\af37\loch\f2         END IF
\par \hich\af2\dbch\af37\loch\f2         
\par \hich\af2\dbch\af37\loch\f2 C      'CALL to CON\hich\af2\dbch\af37\loch\f2 DUCTION ROUTINE'
\par \hich\af2\dbch\af37\loch\f2        CALL TEMP
\par 
\par \hich\af2\dbch\af37\loch\f2        TIME = TIME + TI
\par \hich\af2\dbch\af37\loch\f2        IF (TIME-TP0) 1890,1890,1000
\par \hich\af2\dbch\af37\loch\f2  1000  continue
\par \hich\af2\dbch\af37\loch\f2        DO I = 1,NR
