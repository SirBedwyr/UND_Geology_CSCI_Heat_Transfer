\par \hich\af2\dbch\af37\loch\f2        SUBROUTINE TEMP
\par \hich\af2\dbch\af37\loch\f2       
\par \hich\af2\dbch\af37\loch\f2       C\hich\af2\dbch\af37\loch\f2 OMMON /FIRST/ X,Y,NR,NC,T,TI,NOD
\par \hich\af2\dbch\af37\loch\f2       COMMON /SECOND/ D,CHP,DTC,CHF,TIME
\par \hich\af2\dbch\af37\loch\f2        
\par \hich\af2\dbch\af37\loch\f2        REAL T(120,120), W(120,120),TI,TIME,XB,YB,AD,CT
\par \hich\af2\dbch\af37\loch\f2       REAL X(120),Y(120)
\par \hich\af2\dbch\af37\loch\f2       REAL CHP(9),D(9),QFAC,DTC,CHF
\par \hich\af2\dbch\af37\loch\f2       INTEGER NOD(120,120)
\par \hich\af2\dbch\af37\loch\f2       INTEGER KD, KD1, K\hich\af2\dbch\af37\loch\f2 KD, KKD1, L1, L2, L3, M1, M2, M3, KHP   
\par \hich\af2\dbch\af37\loch\f2       
\par \hich\af2\dbch\af37\loch\f2 C      WRITE(*,*)'ENTERED SUBROUTINE TEMP'
\par \hich\af2\dbch\af37\loch\f2 c     Alas, a magic number appears here.  See comments below for
\par \hich\af2\dbch\af37\loch\f2 c     explanation.  I need to add a data column for density in the
\par \hich\af2\dbch\af37\loch\f2 c     conduction codes.  The\hich\af2\dbch\af37\loch\f2 n this calculation can be done on the fly.
\par \hich\af2\dbch\af37\loch\f2  2720 QFAC = 14.33
\par \hich\af2\dbch\af37\loch\f2 C       DHF = CHF * QFAC * TI 
\par \hich\af2\dbch\af37\loch\f2 C       Temperature difference due to constant heat flow into the model
\par \hich\af2\dbch\af37\loch\f2 c       from the bottom is calculated from DHF.  QFAC is TI (seconds/yr)
\par \hich\af2\dbch\af37\loch\f2 c       divid\hich\af2\dbch\af37\loch\f2 ed by the product of density and heat capacity.  The value
\par \hich\af2\dbch\af37\loch\f2 c       for density is 2200 kg/m^3 and heat capacity is 1000 kJ/kg K.
\par \hich\af2\dbch\af37\loch\f2        DHF = CHF * QFAC *TI
\par \hich\af2\dbch\af37\loch\f2        DO 3760, I = 1, NR
\par \hich\af2\dbch\af37\loch\f2 C      WRITE(*,*) 'ROW',I
\par \hich\af2\dbch\af37\loch\f2       DO 3750, J = 1, NC
\par \hich\af2\dbch\af37\loch\f2       XB = 0.
\par \hich\af2\dbch\af37\loch\f2  \hich\af2\dbch\af37\loch\f2      YB = 0.
\par \hich\af2\dbch\af37\loch\f2 *       KKD is the code for thermal conductivity (1-8)
\par \hich\af2\dbch\af37\loch\f2 *       NNN is the code for direction of heat flow (1-8)
\par \hich\af2\dbch\af37\loch\f2 *       KHP is the code for radioactive heat production (1-8)
\par \hich\af2\dbch\af37\loch\f2 *       the following statements determine KKD, KHP and NNN from \hich\af2\dbch\af37\loch\f2 NOD 
\par \hich\af2\dbch\af37\loch\f2  2780     KKD = NOD(I, J) / 1000
\par \hich\af2\dbch\af37\loch\f2       NNN = NOD(I, J) - KKD * 1000
\par \hich\af2\dbch\af37\loch\f2       KHP = NNN / 100
\par \hich\af2\dbch\af37\loch\f2       NNN = NNN - KHP * 100
\par 
\par \hich\af2\dbch\af37\loch\f2 c       The following computed GO TO has been kept to facilitate
\par \hich\af2\dbch\af37\loch\f2 c       converting the code to FORTRAN 77 if necessary.  Whe\hich\af2\dbch\af37\loch\f2 n time
\par \hich\af2\dbch\af37\loch\f2 c       permits the nested IF's will be converted to SELECT CASE
\par 
\par \hich\af2\dbch\af37\loch\f2 C 2830  GO TO(2850,3320,2910,2880,2850,2850,2910,2880,2910,
\par \hich\af2\dbch\af37\loch\f2 C     + 2880,2850,2910,2880) NNN
\par \hich\af2\dbch\af37\loch\f2       
\par \hich\af2\dbch\af37\loch\f2       IF (NNN.EQ.1) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 2
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.E\hich\af2\dbch\af37\loch\f2 Q.2) THEN
\par \hich\af2\dbch\af37\loch\f2       GOTO 3320
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.3) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.4) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.5) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 2
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.6) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J\hich\af2\dbch\af37\loch\f2  + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 2
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.7) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.8) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.9) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.10) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2  \hich\af2\dbch\af37\loch\f2      NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.11) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 2
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.12) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J + 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       ELSEIF (NNN.EQ.13) THEN
\par \hich\af2\dbch\af37\loch\f2       J1 = J - 1
\par \hich\af2\dbch\af37\loch\f2       NN = 1
\par \hich\af2\dbch\af37\loch\f2       END IF
\par \hich\af2\dbch\af37\loch\f2        
\par \hich\af2\dbch\af37\loch\f2        GO TO 2930
\par \hich\af2\dbch\af37\loch\f2  2880  J1 = \hich\af2\dbch\af37\loch\f2 J - 1
\par \hich\af2\dbch\af37\loch\f2        NN = 1
\par 
\par \hich\af2\dbch\af37\loch\f2 *       routines for diagonally partitioned cells - GONE
\par \hich\af2\dbch\af37\loch\f2        
\par \hich\af2\dbch\af37\loch\f2  2930  KKD1 = NOD(I, J1) / 1000
\par \hich\af2\dbch\af37\loch\f2        IF (KKD - 8) 2950, 2950, 2970       
\par \hich\af2\dbch\af37\loch\f2  2950  KD = KKD
\par \hich\af2\dbch\af37\loch\f2       GO TO 3110
\par \hich\af2\dbch\af37\loch\f2  2970  IF (KKD - 88) 2980, 2980, 3020
\par \hich\af2\dbch\af37\loch\f2  2980  KD = 9
\par \hich\af2\dbch\af37\loch\f2        L2 = KKD / 10
\par \hich\af2\dbch\af37\loch\f2        L3 = KKD - 10 * L2
\par \hich\af2\dbch\af37\loch\f2        IF (J1 - J) 3080, 2950, 3100
\par \hich\af2\dbch\af37\loch\f2  3020  KD = 10
\par \hich\af2\dbch\af37\loch\f2        L1 = KKD / 100
\par \hich\af2\dbch\af37\loch\f2        L1 = KKD - 100 * L1
\par \hich\af2\dbch\af37\loch\f2        L2 = L1 / 10
\par \hich\af2\dbch\af37\loch\f2        L3 = L1 - 10 * L2
\par \hich\af2\dbch\af37\loch\f2        IF (J1 - J) 3100, 2950, 3080
\par \hich\af2\dbch\af37\loch\f2  3080  D(KD) = .2\hich\af2\dbch\af37\loch\f2 5 * D(L2) + .75 * D(L3)
\par \hich\af2\dbch\af37\loch\f2        GO TO 3110
\par \hich\af2\dbch\af37\loch\f2  3100  D(KD) = .75 * D(L2) + .25 * D(L3)
\par \hich\af2\dbch\af37\loch\f2  3110  IF (KKD1 - 8) 3120, 3120, 3140 
\par \hich\af2\dbch\af37\loch\f2  3120  KD1 = KKD1
\par \hich\af2\dbch\af37\loch\f2        GO TO 3280
\par \hich\af2\dbch\af37\loch\f2  3140  IF (KKD1 - 88) 3150,3150, 3190 
\par \hich\af2\dbch\af37\loch\f2  3150  KD1 = 11
\par \hich\af2\dbch\af37\loch\f2        M2 = INT(KKD1 / 10)
\par \hich\af2\dbch\af37\loch\f2        \hich\af2\dbch\af37\loch\f2 M3 = KKD1 - 10 * M2
\par \hich\af2\dbch\af37\loch\f2        IF (J1 - J) 3270, 3120, 3250
\par \hich\af2\dbch\af37\loch\f2  3190  KD1 = 12
\par \hich\af2\dbch\af37\loch\f2        M1 = KKD1 / 100
\par \hich\af2\dbch\af37\loch\f2        M1 = KKD1 - 100 * M1
\par \hich\af2\dbch\af37\loch\f2        M2 = M1 / 10
\par \hich\af2\dbch\af37\loch\f2        M3 = M1 - 10 * M2
\par \hich\af2\dbch\af37\loch\f2        IF (J1 - J) 3250, 3120, 3270
\par \hich\af2\dbch\af37\loch\f2  3250  D(KD1) = .25 * D(M2) + .75 * D(M3)
\par \hich\af2\dbch\af37\loch\f2  \hich\af2\dbch\af37\loch\f2       GO TO 3280
\par \hich\af2\dbch\af37\loch\f2  3270  D(KD1) = .75 * D(M2) + .25 * D(M3)
\par \hich\af2\dbch\af37\loch\f2  3280  CT = T(I, J1) - T(I, J)
\par \hich\af2\dbch\af37\loch\f2        AD = X(J1) / D(KD1) + X(J) / D(KD)
\par \hich\af2\dbch\af37\loch\f2        XB = XB + 2 * CT / (AD * X(J))
\par \hich\af2\dbch\af37\loch\f2        GO TO (3340,2880) NN      
\par \hich\af2\dbch\af37\loch\f2  3320  W(I, J) = T(I, J)
\par \hich\af2\dbch\af37\loch\f2  3340  GO TO(3360,37\hich\af2\dbch\af37\loch\f2 50,3360,3360,3420,3390,3420,3420,3390,3390,3390,
\par \hich\af2\dbch\af37\loch\f2      + 3390,3390)NNN
\par \hich\af2\dbch\af37\loch\f2  3360  I1 = I + 1
\par \hich\af2\dbch\af37\loch\f2        NN = 2
\par \hich\af2\dbch\af37\loch\f2        GO TO 3440
\par \hich\af2\dbch\af37\loch\f2  3390  I1 = I - 1
\par \hich\af2\dbch\af37\loch\f2        NN = 1
\par \hich\af2\dbch\af37\loch\f2        GO TO 3440
\par \hich\af2\dbch\af37\loch\f2  3420  I1 = I + 1
\par \hich\af2\dbch\af37\loch\f2        NN = 1
\par \hich\af2\dbch\af37\loch\f2  3440  KKD1 = NOD(I1, J) / 1000
\par 
\par \hich\af2\dbch\af37\loch\f2 C       W\hich\af2\dbch\af37\loch\f2 RITE(*,*) 'KKD1 AND KD AT THIS POINT ARE', KKD1,KD
\par \hich\af2\dbch\af37\loch\f2        IF (KD - 9) 3500, 3460, 3460
\par \hich\af2\dbch\af37\loch\f2  3460  IF (I1 - I) 3490, 3500, 3470
\par \hich\af2\dbch\af37\loch\f2  3470  D(KD) = .25 * D(L2) + .75 * D(L3)
\par \hich\af2\dbch\af37\loch\f2        GO TO 3500
\par \hich\af2\dbch\af37\loch\f2  3490  D(KD) = .75 * D(L2) + .25 * D(L3)
\par \hich\af2\dbch\af37\loch\f2  3500  IF (KKD1 - 8) 3510, \hich\af2\dbch\af37\loch\f2 3510, 3530
\par \hich\af2\dbch\af37\loch\f2  3510  KD1 = KKD1
\par \hich\af2\dbch\af37\loch\f2 C     WRITE(*,*) 'KD1 AND KKD1', KD1, KKD1
\par \hich\af2\dbch\af37\loch\f2        GO TO 3670
\par \hich\af2\dbch\af37\loch\f2  3530  IF (KKD1 - 88) 3540, 3540, 3580
\par \hich\af2\dbch\af37\loch\f2  3540  M2 = KKD1 / 10
\par \hich\af2\dbch\af37\loch\f2        M3 = KKD1 - 10 * M2
\par \hich\af2\dbch\af37\loch\f2        KD1 = 11
\par \hich\af2\dbch\af37\loch\f2        GO TO 3630
\par \hich\af2\dbch\af37\loch\f2  3580  M1 = KKD1 / 100
\par \hich\af2\dbch\af37\loch\f2        M1 =\hich\af2\dbch\af37\loch\f2  KKD1 - 100 * M1
\par \hich\af2\dbch\af37\loch\f2        M2 = M1 / 10
\par \hich\af2\dbch\af37\loch\f2        M3 = M1 - 10 * M2
\par \hich\af2\dbch\af37\loch\f2        KD1 = 12
\par \hich\af2\dbch\af37\loch\f2  3630  IF (I1 - I) 3640, 3510, 3660
\par \hich\af2\dbch\af37\loch\f2  3640  D(KD1) = .25 * D(M2) + .75 * D(M3)
\par \hich\af2\dbch\af37\loch\f2        GO TO 3670
\par \hich\af2\dbch\af37\loch\f2  3660  D(KD1) = .75 * D(M2) + .25 * D(M3)
\par \hich\af2\dbch\af37\loch\f2  3670  CT = T(I1, J) - T(I, J)
\par \hich\af2\dbch\af37\loch\f2 C        write(*,*) 'number  D(KD1)  D(KD)  KD1  KD  NN' 
\par \hich\af2\dbch\af37\loch\f2 C     write(*,*) '3670', D(KD1), D(KD), KD1, KD,NN
\par \hich\af2\dbch\af37\loch\f2       IF (KD1.LE.1) THEN KD1 = 1 
\par \hich\af2\dbch\af37\loch\f2       AD = Y(I1) / D(KD1) + Y(I) / D(KD)
\par \hich\af2\dbch\af37\loch\f2 C     WRITE(*,*) AD,Y(I1),Y(I)
\par \hich\af2\dbch\af37\loch\f2       YB = YB + 2 * CT / (AD * Y(I))
\par \hich\af2\dbch\af37\loch\f2 C     WRITE(*,*) YB,' VALUE FOR YB'
\par \hich\af2\dbch\af37\loch\f2        GO TO(3710,3390) NN
\par \hich\af2\dbch\af37\loch\f2  3710  W(I, J) = T(I, J) + TI * (XB + YB)
\par \hich\af2\dbch\af37\loch\f2        IF (NNN-10) 3740,3740, 3730
\par \hich\af2\dbch\af37\loch\f2  3730  W(I, J) = W(I, J) + DHF / Y(I)
\par \hich\af2\dbch\af37\loch\f2  3740  W(I, J) = W(I, J) + CHP(KHP) * TI / DTC
\par \hich\af2\dbch\af37\loch\f2  3750  CONTINUE
\par \hich\af2\dbch\af37\loch\f2  3760 \hich\af2\dbch\af37\loch\f2  CONTINUE
\par \hich\af2\dbch\af37\loch\f2      
\par \hich\af2\dbch\af37\loch\f2        DO 3810, I = 1, NR
\par \hich\af2\dbch\af37\loch\f2       DO 3800, J = 1, NC
\par \hich\af2\dbch\af37\loch\f2       T(I, J) = W(I, J)
\par \hich\af2\dbch\af37\loch\f2  3800  CONTINUE  
\par \hich\af2\dbch\af37\loch\f2  3810  CONTINUE
\par \hich\af2\dbch\af37\loch\f2       RETURN
\par \hich\af2\dbch\af37\loch\f2       END
